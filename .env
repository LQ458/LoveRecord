# AI Service Configuration
AI_PROVIDER=dashscope

# DashScope (阿里云通义千问)
DASHSCOPE_API_KEY=sk-9a0a426475814fc48ef9ae955be93530

# Baidu ERNIE Bot (百度文心一言)
BAIDU_CLIENT_ID=your_baidu_client_id_here
